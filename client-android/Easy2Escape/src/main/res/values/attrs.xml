<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="ProgressWheel">
    <attr name="text" format="string" />
    <attr name="textColor" format="color" />
    <attr name="textSize" format="dimension" />
    <attr name="barColor" format="color" />
    <attr name="rimColor" format="color" />
    <attr name="rimWidth" format="dimension" />
    <attr name="spinSpeed" format="dimension" />
    <attr name="delayMillis" format="integer" />
    <attr name="circleColor" format="color" />
    <attr name="wheelradius" format="dimension" />
    <attr name="barWidth" format="dimension" />
    <attr name="barLength" format="dimension" />
    <attr name="contourColor" format="color"/>
    <attr name="contourSize" format="dimension"/>
    <attr name="title"/>
  </declare-styleable>
  <declare-styleable name="MatrialProgressWheel">
    <attr name="progressIndeterminate" format="boolean" />
    <attr name="barColor"/>
    <attr name="rimColor"/>
    <attr name="rimWidth"/>
    <attr name="spinSpeed"/>
    <attr name="barSpinCycleTime" format="integer" />
    <attr name="circleRadius" format="dimension" />
    <attr name="fillRadius" format="boolean" />
    <attr name="barWidth"/>
    <attr name="linearProgress" format="boolean" />
  </declare-styleable>
  <declare-styleable name="ProgressImageView">
    <attr name="round" format="boolean" />
    <attr name="cornerRadius" format="dimension" />
    <attr name="source" format="reference"/>
  </declare-styleable>
</resources>
